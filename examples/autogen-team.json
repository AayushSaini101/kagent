{
    "provider": "autogen_agentchat.teams.RoundRobinGroupChat",
    "component_type": "team",
    "version": 1,
    "component_version": 1,
    "description": null,
    "config": {
        "participants": [
            {
                "provider": "autogen_agentchat.agents.AssistantAgent",
                "component_type": "agent",
                "version": 1,
                "component_version": 1,
                "config": {
                    "name": "assistant",
                    "model_client": {
                        "provider": "autogen_ext.models.openai.OpenAIChatCompletionClient",
                        "component_type": "model",
                        "version": 1,
                        "component_version": 1,
                        "config": {"model": "gpt-4o"}
                    },
                    "tools": [
                      {
                        "provider": "autogen_ext.tools.http.HttpTool",
                        "component_type": "tool",
                        "version": 1,
                        "component_version": 1,
                        "config": {
                          "name": "get_pods",
                          "description": "Get pods from the Kubernetes cluster",
                          "scheme": "http",
                          "host": "localhost",
                          "port": 8000,
                          "path": "/k8s/get_pods",
                          "method": "POST",
                          "headers": {
                            "Content-Type": "application/json"
                          },
                          "json_schema": {
                            "type": "object",
                            "properties": {
                              "ns": {"type": "string", "description": "The namespace of the pods to get information about"},
                              "all_namespaces": {"type": "boolean", "description": "Whether to get pods from all namespaces"},
                              "output": {"type": "string", "description": "The output format of the pod information"}
                            },
                            "required": ["ns", "all_namespaces", "output"]
                          }
                        }
                      },
                      {
                        "provider": "kagent.tools.k8s.GetPods",
                        "component_type": "tool",
                        "version": 1,
                        "component_version": 1,
                        "config": {}
                      }
                    ],
                    "model_context": {
                        "provider": "autogen_core.model_context.UnboundedChatCompletionContext",
                        "component_type": "chat_completion_context",
                        "version": 1,
                        "component_version": 1,
                        "config": {}
                    },
                    "description": "An agent that provides assistance with ability to use tools.",
                    "system_message": "Use tools to solve tasks.",
                    "reflect_on_tool_use": false,
                    "tool_call_summary_format": "{result}"
                }
            }
        ],
        "termination_condition": {
            "provider": "autogen_agentchat.conditions.MaxMessageTermination",
            "component_type": "termination",
            "version": 1,
            "component_version": 1,
            "config": {"max_messages": 2}
        }
    }
}
